    "@vuelidate/validators": "^2.0.0",
    "axios": "^0.26.1",
    "bootstrap": "^4.6.1",
    "core-js": "^3.8.3",
    "dayjs": "^1.11.7",
    "jquery": "^3.6.0",
    "pinia": "^2.0.30",
    "popper.js": "^1.16.1",
    "qs": "^6.11.0",
    "v-calendar": "^3.0.0-alpha.8",
    "vee-validate": "^4.5.10",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.2",
